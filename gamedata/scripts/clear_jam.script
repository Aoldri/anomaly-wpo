-- default way of going aboot it is alt+reload
function on_key_release(key)
	
	local bind = dik_to_bind(key)
	if bind == key_bindings.kWPN_RELOAD and arti_jamming.d_flag then
		arti_jamming.unjam()
    end
end


function on_game_start()
    RegisterScriptCallback("on_key_release",on_key_release)
end